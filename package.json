{
  "name": "periodo-client",
  "version": "0.1.0",
  "author": "Patrick Golden",
  "description": "Client for browsing and editing PeriodO data.",
  "private": true,
  "dependencies": {
    "Dexie": "git://github.com/ptgolden/Dexie.js#package.json",
    "backbone": "^1.1.2",
    "backbone-relational": "^0.8.8",
    "backbone.stickit": "^0.8.0",
    "d3": "^3.4.8",
    "deep-equal": "^0.2.1",
    "fast-json-patch": "^0.4.0",
    "jquery": "^1.11.0",
    "json-stable-stringify": "^1.0.0",
    "jsonld": "^0.3.4",
    "less": "^1.7.3",
    "n3": "^0.2.7",
    "pegjs": "^0.8.0",
    "spin.js": "^2.0.1",
    "tablesort": "^2.0.1",
    "traverse": "^0.6.6",
    "underscore": "^1.6.0",
    "watch-lessc": "0.0.2"
  },
  "devDependencies": {
    "brfs": "1.0.0",
    "browserify": "^4.1.2",
    "browserify-shim": "^3.5.0",
    "mocha": "^1.18.0",
    "node-underscorify": "0.0.9",
    "serve": "^1.4.0",
    "testling": "^1.6.1",
    "uglify-js": "^2.4.16",
    "watchify": "^0.9.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "node-underscorify",
      "brfs"
    ]
  },
  "browser": {
    "jquery-bootstrap": "./lib/bootstrap.js",
    "jquery-typeahead": "./lib/typeahead.bundle.js"
  },
  "browserify-shim": {
    "jquery-bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "jquery-typeahead": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "testling": {
    "harness": "mocha",
    "files": "src/tests/*.js",
    "browsers": [
      "firefox/24..latest",
      "chrome/latest",
      "ie/10..latest",
      "safari/6..latest",
      "opera/latest",
      "ipad/6..latest"
    ]
  },
  "scripts": {
    "test": "mocha -R spec src/tests/*.js",
    "start": "serve -p 8000"
  }
}
