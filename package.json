{
  "name": "periodo-client",
  "version": "0.6.12",
  "author": "Patrick Golden",
  "description": "Client for browsing and editing PeriodO data.",
  "private": true,
  "dependencies": {
    "backbone": "^1.1.2",
    "concat-stream": "^1.4.8",
    "csv-write-stream": "^0.3.0",
    "d3": "^3.4.8",
    "dexie": "^1.0.4",
    "diff-match-patch": "^1.0.0",
    "fast-json-patch": "^0.5.2",
    "filesaver.js": "^0.1.1",
    "immfacet": "^0.5.0",
    "immpatch": "^0.2.0",
    "immutable": "^3.7.2",
    "iso-639-3": "^0.2.0",
    "javascript-natural-sort": "^0.7.1",
    "jquery": "^1.11.0",
    "json-pointer": "^0.3.0",
    "json-stable-stringify": "^1.0.0",
    "jsonld": "^0.3.4",
    "n3": "^0.2.7",
    "pegjs": "^0.8.0",
    "react": "^0.13.3",
    "spark-md5": "0.0.5",
    "spin.js": "^2.0.1",
    "tablesort": "^3.0.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "brfs": "1.0.0",
    "browserify": "^10.1.3",
    "browserify-shim": "^3.8.2",
    "dom-sandbox": "0.0.1",
    "exorcist": "^0.4.0",
    "less": "^1.7.3",
    "mocha": "^1.18.0",
    "mochify": "^2.8.1",
    "node-underscorify": "0.0.9",
    "phantomjs": "^1.9.16",
    "testling": "^1.6.1",
    "uglify-js": "^2.4.19",
    "watchify": "^3.2.1",
    "watch-lessc": "0.0.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "sourceMapRelative": ".",
          "only": "src/**"
        }
      ],
      "browserify-shim",
      "node-underscorify",
      "brfs"
    ]
  },
  "browser": {
    "jquery-bootstrap": "./lib/bootstrap.js",
    "jquery-typeahead": "./lib/typeahead.bundle.js"
  },
  "browserify-shim": {
    "jquery-bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "jquery-typeahead": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "testling": {
    "harness": "mocha",
    "files": "src/tests/*.js",
    "browsers": [
      "firefox/24..latest",
      "chrome/latest",
      "ie/10..latest",
      "safari/6..latest",
      "opera/latest",
      "ipad/6..latest"
    ]
  },
  "scripts": {
    "test": "mochify -R spec ./src/tests/*.js",
    "test-watch": "mochify -w -R spec ./src/tests/*.js",
    "start": "serve -p 8000"
  }
}
