{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "curly": [2, "multi-line"],
    "new-cap": 0,
    "no-mixed-requires": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "quotes": 0,
    "semi": 0
  }
}
